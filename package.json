{
  "name": "markup-template",
  "engines": {
    "node": "^16.14.0",
    "npm": "^8.5.3"
  },
  "type": "module",
  "scripts": {
    "tsc": "tsc src/ts/*.ts --noEmit",
    "eslint": "eslint 'src/ts/*.ts' --fix",
    "lint": "npm run tsc && npm run eslint",
    "start": "NODE_ENV=start node --loader ts-node/esm build.ts",
    "build": "NODE_ENV=build node --loader ts-node/esm build.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "esbuild": "^0.14.25",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "pug-cli": "github:pugjs/pug-cli#master",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  }
}
